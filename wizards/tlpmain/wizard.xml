<?xml version="1.0" encoding="UTF-8"?>
<!--
Custom project wizard configuration example file. Note that by convention, 
the project file goes last.
The "class" and "firstpage" attributes specify that it is a Qt 4 wizard and 
leave room for the Qt 4 target page.
-->
<wizard version="1" kind="project"
        class="qt4project" firstpage="10"
        id="Tulip.Main" category="Tulip.Main">
    <icon>logo.png</icon>
    <description>Creates a simple tulip application loading all existing plugins</description>
    <displayname>Tulip Application</displayname>;
    <displaycategory>Tulip Application</displaycategory>
    <files>
        <file source="main.cpp"   target="%ProjectName:l%.%CppSourceSuffix%" openeditor="true"/>
        <file source="main.cmake"     target="CMakeLists.txt" openproject="true" openeditor="false"/>
    </files>

</wizard>
